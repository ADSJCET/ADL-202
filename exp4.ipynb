    "from math import pi\n",
    "\n",
    "r = int(input(\"Enter Radius \"))\n",
    "h = int(input(\"Enter Height \"))\n",
    "\n",
    "vol = pi * r * r * h\n",
    "sf = 2 * pi * r * (r + h)\n",
    "\n",
    "print(\"Volume\", vol)\n",
    "print(\"Surface Area\", sf)"
